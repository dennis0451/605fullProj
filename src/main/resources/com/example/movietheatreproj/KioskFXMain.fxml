<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="mainAnchor" onDragDetected="#quitPressed" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.movietheatreproj.KioskController">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="40.0" layoutY="49.0" prefHeight="39.0" prefWidth="270.0" text="Movie Showtimes">
         <font>
            <Font name="Baskerville Old Face" size="24.0" />
         </font>
      </Label>
      <Button fx:id="todaySelections" layoutX="107.0" layoutY="109.0" mnemonicParsing="false" onAction="#movieSelectionsToday" prefHeight="22.0" prefWidth="125.0" text="Today's Showings">
         <font>
            <Font name="Baskerville Old Face" size="12.0" />
         </font>
      </Button>
      <Button fx:id="sodaButton" layoutX="382.0" layoutY="109.0" mnemonicParsing="false" onAction="#sodaScene" prefHeight="22.0" prefWidth="125.0" text="Soda Selections">
         <font>
            <Font name="Baskerville Old Face" size="12.0" />
         </font>
      </Button>
      <Button fx:id="quitButton" layoutX="516.0" layoutY="360.0" mnemonicParsing="false" onAction="#quitPressed" text="Dev-Quit">
         <font>
            <Font name="Baskerville Old Face" size="13.0" />
         </font>
      </Button>
      <Label fx:id="processingLabel" layoutX="450.0" layoutY="320.0" prefHeight="25.0" prefWidth="99.0">
         <font>
            <Font name="Baskerville Old Face" size="12.0" />
         </font>
      </Label>
      <Label fx:id="titleLabel1" alignment="CENTER" layoutX="300.0" layoutY="49.0" prefHeight="39.0" prefWidth="270.0" text="Concessions">
         <font>
            <Font name="Baskerville Old Face" size="24.0" />
         </font>
      </Label>
      <Button fx:id="futureSelections" layoutX="107.0" layoutY="145.0" mnemonicParsing="false" onAction="#movieSelectionsFuture" prefHeight="22.0" prefWidth="125.0" text="Future Showings">
         <font>
            <Font name="Baskerville Old Face" size="12.0" />
         </font>
      </Button>
      <Button fx:id="popcornSelections" layoutX="382.0" layoutY="145.0" mnemonicParsing="false" onAction="#popcornScene" prefHeight="22.0" prefWidth="125.0" text="Popcorn Selections">
         <font>
            <Font name="Baskerville Old Face" size="12.0" />
         </font>
      </Button>
      <Button fx:id="checkOutButton" layoutX="266.0" layoutY="235.0" mnemonicParsing="false" onAction="#cartScene" prefHeight="39.0" prefWidth="88.0" text="Check out" />
   </children>
</AnchorPane>
